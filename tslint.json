{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],

  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "jsxSingleQuote": true
      }
    ],
    "import-name": false,
    "variable-name": false,
    "no-console": false,
    "quotemark": [true, "single"],
    "interface-name": false
  }
}
